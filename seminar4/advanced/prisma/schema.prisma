generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Content {
  id         Int     @id @default(autoincrement())
  title      String? @db.VarChar(400)
  actor      String? @db.VarChar(300)
  genre      String? @db.VarChar(300)
  year       Int?
  age        Int?
  episodeNum Int?
  image      String? @db.VarChar(500)
}
